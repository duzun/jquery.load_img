{
  "name": "jquery.load_img",
  "title": "Load images asynchronously",
  "description": "Load images asynchronously and cache them.",
  "version": "1.5.2",
  "homepage": "https://github.com/duzun/jquery.load_img",
  "module": "load_img.js",
  "jsnext:main": "load_img.js",
  "esnext": "load_img.js",
  "main": "dist/load_img.js",
  "unpkg": "dist/load_img.min.js",
  "author": {
    "name": "Dumitru Uzun",
    "email": "contact@duzun.me",
    "url": "https://DUzun.Me"
  },
  "reveal": true,
  "repository": {
    "type": "git",
    "url": "git://github.com/duzun/jquery.load_img.git"
  },
  "bugs": "https://github.com/duzun/jquery.load_img/issues",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/duzun/jquery.load_img/blob/master/LICENSE-MIT"
    }
  ],
  "scripts": {
    "build": "npx rollup -c rollup.config.js ",
    "verup": "verup"
